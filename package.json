{
  "name": "fortune-teller",
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.5",
    "babel-jest": "^29.5.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "jest-puppeteer": "^9.0.0",
    "jsdoc": "^4.0.3",
    "prettier": "^2.8.8",
    "puppeteer": "^20.5.0"
  },
  "scripts": {
    "format": "prettier --no-config -w .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "docs": "jsdoc . -r -c .jsdoc.conf.json -d ./JSDOCs/"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "preset": "jest-puppeteer",
    "verbose": true,
    "testPathIgnorePatterns": [
      "/__tests__/consts.js"
    ]
  },
  "dependencies": {
    "@barba/core": "^2.9.7",
    "animejs": "^3.2.2",
    "gsap": "^3.12.5",
    "python3": "^0.0.1",
    "vanta": "^0.5.24"
  }
}
